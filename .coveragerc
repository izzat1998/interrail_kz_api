[run]
source = apps
omit =
    */migrations/*
    */__init__.py
    */apps.py
    */admin.py
    */management/commands/*
    */urls.py
    */asgi.py
    */wsgi.py
    */settings.py
    */conftest.py
    */tests/*
    */test_*.py
    */*_test.py
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
skip_empty = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
